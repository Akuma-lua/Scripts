local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Titty hub  " .. Fluent.Version,
    SubTitle = "Enjoy",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false,
    Theme = "Amethyst",
    MinimizeKey = Enum.KeyCode.K
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "" }),
    Player = Window:AddTab({ Title = "Player", Icon = "" }),
}

Tabs.Player:AddSlider("WalkSpeedSlider", {
    Title = "Walk speed",
    Default = 16,
    Min = 0,
    Max = 999,
    Rounding = 1,
    Callback = function(Value)
    local speed = tonumber(Value)
        if speed then
            game.Players.LocalPlayer:SetAttribute("CurrentSpeed", speed)
        end
    end
})

Tabs.Player:AddSlider("JumpPowerSlider", {
    Title = "Jump Power",
    Default = 50,
    Min = 0,
    Max = 300,
    Rounding = 1,
    Callback = function(Value)
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChildOfClass("Humanoid") then
            local hum = char:FindFirstChildOfClass("Humanoid")
            hum.UseJumpPower = true
            hum.JumpPower = Value
        end
    end
})

    Tabs.Player:AddButton({
        Title = "No view limit",
        Description = "",
        Callback = function()
                 game.Players.LocalPlayer.CameraMaxZoomDistance = 999999
        end
    })

        Tabs.Player:AddButton({
        Title = "Get double speed",
        Description = "",
        Callback = function()
                 game.Players.LocalPlayer:SetAttribute("HasDoubleSpeed", true)
        end
    })

        Tabs.Player:AddButton({
        Title = "Get speed boost",
        Description = "",
        Callback = function()
                 game.Players.LocalPlayer:SetAttribute("HasSpeedBoost", true)
        end
    })

        Tabs.Player:AddButton({
        Title = "Get super speed boost",
        Description = "",
        Callback = function()
                 game.Players.LocalPlayer:SetAttribute("HasSuperSpeedBoost", true)
        end
    })

Tabs.Main:AddToggle("Transparent Tsunami", {Title = "Transparent tsunami", Default = false}):OnChanged(function(state)
    if not state then return end
    local function apply(obj)
        if obj:IsA("Model") then
            obj:SetAttribute("Speed", 0)
        elseif obj.Name == "TsunamiWave" then
            obj.Transparency = 0.7
        elseif obj.Name == "Hitbox" then
            obj:Destroy()

        end
    end
    for _,v in ipairs(workspace.ActiveTsunamis:GetDescendants()) do
        apply(v)
    end

    workspace.ActiveTsunamis.DescendantAdded:Connect(function(v)
        apply(v)
    end)
end)

    local function applyinstantsteal()
    for _,v in ipairs(game.workspace.ActiveBrainrots:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            if v.HoldDuration == 0.5 then
                v.HoldDuration = 0
            end
            v.HoldDuration = 0
        end
    end
    game.workspace.ActiveBrainrots.DescendantAdded:Connect(function(v)
        if v:IsA("ProximityPrompt") then
            if v.HoldDuration == 0.5 then
                v.HoldDuration = 0
            end
                v.HoldDuration = 0
            end
        end)
    end 

local brainrotConnection

Tabs.Main:AddToggle("InstantStealToggle", {Title = "Instant steal", Default = false}):OnChanged(function(state)
    if state then
        applyinstantsteal()
        brainrotConnection = workspace.ActiveBrainrots.DescendantAdded:Connect(function(v)
            if v:IsA("ProximityPrompt") then
                v.HoldDuration = 0
            end
        end)
    else
        if brainrotConnection then
            brainrotConnection:Disconnect()
        end

        for _, v in ipairs(workspace.ActiveBrainrots:GetDescendants()) do
            if v:IsA("ProximityPrompt") then
                v.HoldDuration = 0.5
            end
        end
    end
end)
local Vim = game:GetService("VirtualInputManager")
local antiAfkRunning = false
local antiAfkThread
local basesFolder = workspace:WaitForChild("Bases_NEW")

Tabs.Main:AddToggle("Anti afk", {
	Title = "Anti AFK",
	Default = false
}):OnChanged(function(state)
	if state then
		if antiAfkRunning then return end
		antiAfkRunning = true

		antiAfkThread = task.spawn(function()
			while antiAfkRunning do
				Vim:SendKeyEvent(true, Enum.KeyCode.Space, false, game)
				task.wait(0.1)
				Vim:SendKeyEvent(false, Enum.KeyCode.Space, false, game)
				task.wait(600)
			end
		end)
	else
		antiAfkRunning = false
	end
end)

local player = game.Players.LocalPlayer
local ownedBase = nil

for _, base in ipairs(basesFolder:GetChildren()) do
	local holder = base:GetAttribute("Holder")
	if holder and holder == player.UserId then
		ownedBase = base
		break
	end
end

local player = game.Players.LocalPlayer
local running = false
local thread

local player = game.Players.LocalPlayer
local running = false
local thread

local function getAllCollects(base)
	local collects = {}

	for _, v in ipairs(base:GetDescendants()) do
		if v.Name == "Collect" then
			if v:IsA("BasePart") or v:IsA("Model") then
				table.insert(collects, v)
			end
		end
	end

	return collects
end

Tabs.Main:AddToggle("collect money", {
	Title = "Auto collect money",
	Default = false
}):OnChanged(function(state)
	running = state

	if state then
		if thread then return end

		thread = task.spawn(function()
			while running do
				local char = player.Character
				local hrp = char and char:FindFirstChild("HumanoidRootPart")
				if not hrp or not ownedBase then
					task.wait(0.2)
					continue
				end

				local collects = getAllCollects(ownedBase)

				for _, v in ipairs(collects) do
					if not running then break end

					if v:IsA("BasePart") then
						hrp.CFrame = v.CFrame + Vector3.new(0, 7, 0)
					elseif v:IsA("Model") then
						hrp.CFrame = v:GetPivot() + Vector3.new(0, 7, 0)
					end

					task.wait(0.5)
				end

				task.wait(1)
			end
		end)
	else
		thread = nil
	end
end)

local radioactiverunning = false
local radioactiveThread

Tabs.Main:AddToggle("radioactivecoins", {
	Title = "Auto collect radioactive coins",
	Default = false
}):OnChanged(function(state)
	if state then
		if radioactiverunning then return end
		radioactiverunning = true

		radioactiveThread = task.spawn(function()
                    local player = game.Players.LocalPlayer
        local char = player.Character or player.CharacterAdded:Wait()
        local folder = workspace.EventParts

for _, v in ipairs(folder:GetChildren()) do
	if v:IsA("Model") then
		v:PivotTo(char:GetPivot())
	        elseif v:IsA("BasePart") then
		            v.CFrame = char:GetPivot()
	            end
            end
		end)
	else
        radioactiveThread = nil
	end
end)
